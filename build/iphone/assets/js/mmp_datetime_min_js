var MMPDate=function(e){function t(){return g=new Date}function i(){return g.getFullYear()}function n(){return g.getMonth()}function s(){return g.getDay()}function a(){return g.getDate()}function o(){return g.getHours()}function r(){return g.getMinutes()}function u(){return g.getSeconds()}function l(){return g.getMilliseconds()}function h(e){if(void 0!=e&&null!=e&&""!=e){for(var t=e.length,i=0;t>i;i++){var n=e.charAt(i);if(0==_.length)_.push(n);else{var s=_.pop();if(_.push(s),s==n)_.push(n);else{var a=_.join("");d(a),_=[],_.push(n)}}}var a=_.join("");d(a),_=[]}else m=g.toString()}function d(e){var t="";switch(e){case"YYYY":t=i();break;case"YYY":var h=i();t=h-2e3;break;case"MMMMM":var d=n();t=p[d];break;case"MMMM":var d=n();t=c[d];break;case"MMM":var d=n();t=d+1;break;case"DDDDD":var g=s();t=f[g];break;case"DDDD":var g=s();t=v[g];break;case"DDD":var g=a();t=g;break;case"HHHH":var _=o();t=_;break;case"HHH":var _=o();t=_>12?_-12:_;break;case"mmm":t=r();break;case"sss":t=u();break;case"lll":t=l();break;default:t=e}m+=t}{var c=["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"],p=["January","February","March","April","May","June","July","August","September","October","November","December"],v=["Sun","Mon","Tue","Wed","Thu","Fri","Sat"],f=["Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday"],g=null,m="",_=[];!function(){t(),h(e)}()}this.FormattedDate=function(){return m}};