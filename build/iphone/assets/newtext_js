Ti.App.removeEventListener("webviewEvent",function(){}),Ti.App.addEventListener("webviewEvent",function(e){"\b_"==e.text?(e.text="",contentTyped=contentTyped.slice(0,-1),txtViewDesc.value=contentTyped+"_"):(contentTyped.length!=txtViewDesc.value.length-1&&(contentTyped=txtViewDesc.value),contentTyped+=e.text,e.text="",Titanium.App.keyboardVisible||(txtViewDesc.value=contentTyped+"_")),trailer=contentTyped.slice(-23),Ti.API.info("trailer: "+trailer),aTrailer.value=trailer});