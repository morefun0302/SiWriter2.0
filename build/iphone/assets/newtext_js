Ti.App.removeEventListener("webviewEvent",function(){}),Ti.App.addEventListener("webviewEvent",function(e){"\b_"==e.text&&(e.text="",contentTyped=contentTyped.slice(0,-1),trailer=trailer.slice(0,-1),txtViewDesc.value=contentTyped+"_"),contentTyped.length!=txtViewDesc.value.length-1&&(contentTyped=txtViewDesc.value),contentTyped+=e.text,Titanium.App.keyboardVisible||(txtViewDesc.value=contentTyped+"_"),trailer=trailer.slice(-23)+e.text,aTrailer.value=trailer+"_"});